<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <!--Do not use Project Properties customizer when editing this file manually.-->
            <name>JSF Templating</name>
            <properties>
                <property-file>build.properties</property-file>
            </properties>
            <folders>
                <source-folder>
                    <label>src\java</label>
                    <type>java</type>
                    <location>src/java</location>
                </source-folder>
                <source-folder>
                    <label>samples/editor/src/conf</label>
                    <type>java</type>
                    <location>samples/editor/src/conf</location>
                </source-folder>
                <source-folder>
                    <label>samples/editor/src/java</label>
                    <type>java</type>
                    <location>samples/editor/src/java</location>
                </source-folder>
                <source-folder>
                    <label>samples/editor/web</label>
                    <type>java</type>
                    <location>samples/editor/web</location>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <target>all</target>
                </action>
                <action name="clean">
                    <target>clean</target>
                </action>
                <action name="rebuild">
                    <target>clean</target>
                    <target>all</target>
                </action>
                <action name="compile.single">
                    <script>nbproject/ide-file-targets.xml</script>
                    <target>compile-selected-files-in-java</target>
                    <context>
                        <property>files</property>
                        <folder>src/java</folder>
                        <pattern>\.java$</pattern>
                        <format>relative-path</format>
                        <arity>
                            <separated-files>,</separated-files>
                        </arity>
                    </context>
                </action>
            </ide-actions>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>src\java</label>
                        <location>src/java</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>samples/editor/src/conf</label>
                        <location>samples/editor/src/conf</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>samples/editor/src/java</label>
                        <location>samples/editor/src/java</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>samples/editor/web</label>
                        <location>samples/editor/web</location>
                    </source-folder>
                    <source-file>
                        <location>build.xml</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="clean"/>
                    <ide-action name="rebuild"/>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/1">
            <compilation-unit>
                <package-root>src/java</package-root>
                <classpath mode="compile">../as90/publish/glassfishPE/lib/javaee.jar</classpath>
                <source-level>1.5</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>samples/editor/src/conf</package-root>
                <source-level>1.5</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>samples/editor/src/java</package-root>
                <classpath mode="compile">dist/jsftemplating-dt.jar:dist/jsftemplating.jar:samples/lib/dataprovider.jar:samples/lib/defaulttheme.jar:samples/lib/webui.jar</classpath>
                <source-level>1.5</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>samples/editor/web</package-root>
                <source-level>1.5</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
